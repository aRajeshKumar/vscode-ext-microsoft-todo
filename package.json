{
  "name": "todo",
  "displayName": "ToDo",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.45.0"
  },
  "typings": "out/index",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:todoView"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "todo.helloWorld",
        "title": "Hello World",
        "icon": {
          "light": "resources/dark/logo.png",
          "dark": "resources/dark/logo.png"
        }
      },
      {
        "command": "todoView.refreshEntry",
        "title": "Refresh",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      }
    ],
    "views": {
      "package-explorer": [
        {
          "id": "todoView",
          "name": "To Do"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "package-explorer",
          "title": "To Do",
          "icon": "resources/dark/logo.png"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.45.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "eslint": "^6.8.0",
    "@typescript-eslint/parser": "^2.30.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "@types/lodash": "^4.14.158",
    "axios": "^0.19.2",
    "lodash": "^4.17.19",
    "request": "^2.88.2"
  }
}
